  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor {
}

.floor_group {
}
.floor_title{
  padding: 10rpx 0;
}
.floor_title image {
  width: 100%;
}

.floor_list navigator {
  float: left;
  width: 33.33%;
}

.floor_list image {
  width: 100%;
  height: 100%;
}

/* 后四个超链接 */

.floor_list:nth-last-child(-n+4) image {
  height: 26vw;
  border-left: 10rpx solid #fff;
}
.floor_list:nth-child(2) image {border-bottom: 10rpx solid #fff;}
.floor_list:nth-child(3) image {border-bottom: 10rpx solid #fff;}